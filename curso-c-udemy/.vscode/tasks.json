{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compilar C Programa20",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-Wall",
                "-Wextra",
                "-g3",
                "${workspaceFolder}/aulas/programa20.c",
                "${workspaceFolder}/aulas/ajuda.c",
                "-o",
                "${workspaceFolder}/aulas/output/programa20.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"],
            "detail": "Compilação de programa20 com ajuda.c"
        }
    ]
}
